<style type="text/css"> 
body, div, p, h1, h2, h3, fieldset, ul, li, dl, dt, dd { padding:0; margin:0; }
body { background:#fff url(http://i1.daumcdn.net/icon/error/error_top_bg.gif) repeat-x; text-align:center; }
ul, li, dl, dt, dd { list-style:none; }
h1 { display:none; }
 
#topArea { width:538px; background:url(http://i1.daumcdn.net/icon/error/error_bar.gif) no-repeat bottom; margin:68px auto 45px auto; padding-bottom:6px; height:35px; }
#topArea a.daum { background:url(http://i1.daumcdn.net/icon/2010_global/daumlogo_bg.gif) no-repeat; width:62px; height:26px; display:block; text-indent:-10000em; float:left;margin:0 0 0 9px;}
#topArea #serviceLink { float:right; margin:17px 3px 0 0; }
#topArea #serviceLink li { float:left; }
#topArea #serviceLink li.dot { background:url(http://i1.daumcdn.net/icon/error/error_m.gif) no-repeat; padding-left:8px; }
#topArea #serviceLink a.lnk_daum { background:url(http://i1.daumcdn.net/icon/error/error_m1.gif) no-repeat; width:63px; height:12px; display:block; text-indent:-10000em; }
#topArea #serviceLink a.lnk_cs { background:url(http://i1.daumcdn.net/icon/error/error_m2.gif) no-repeat; width:43px; height:12px; display:block; text-indent:-10000em; }
#content, #footTxt { width:428px; margin:0 auto; text-align:left; }
#content h3 { background:url(http://i1.daumcdn.net/icon/error/error_ic.gif) no-repeat; padding:8px 0 0 70px; font-size:18px; font-family:dotum,돋움,sans-serif; letter-spacing:-1px; color:#333; line-height:25px; height:45px; margin-bottom:34px; }
#content p { font-size:12px; font-family:dotum,돋움,sans-serif; color:#666; line-height:18px; margin-bottom:10px; }
#content p a { font-weight:bold; color:#576FE6; text-decoration:underline; }
#footTxt { clear:both;width:360px;margin:10px auto; font-size:11px; font-family:dotum,돋움,sans-serif; color:#8C9AFF;}
 
#bottomArea { width:518px; margin:85px auto 50px auto; border-top:1px #E0E4F0 solid; padding-top:11px; font-size:11px; font-family:Tahoma; color:#999; }
#bottomArea a { color:#999; text-decoration:none; }
#bottomArea a:hover { text-decoration:underline; }
 
/* 검색 */
#tsch_area { width:345px; height:35px; margin:26px auto 0 auto; text-align:left; }
#tsch_area h2, #tsch_area legend { display:none; }
#tsch_area fieldset {border:0 none;}
 
#daumSearch {color:#000}
#daumSearch .searchbar {background:url(http://i1.daumcdn.net/icon/2010_global/bgSearch2.gif) no-repeat;width:265px;height:31px;float:left;display:inline; margin-right:4px}
#daumSearch input.search {width:233px;background-color:transparent;color:#000;border:none 0;margin:4px;padding:4px 4px 3px 4px;float:left;display:inline; height:18px;outline:none;font:12px/1.5 dotum,sans-serif}
#daumNoSuggest {background:url(http://i1.daumcdn.net/icon/2010_global/icoSuggest.gif) no-repeat;width:16px;height:17px;position:relative;margin:7px -3px 0 0;border:none;float:left;display:inline;}
#daumBtnSearchAll {background:url(http://i1.daumcdn.net/icon/2010_global/btnSearch2.gif) no-repeat 0 0;width:57px;height:31px;border:none;text-indent:-9999in;overflow:hidden; cursor:pointer;vertical-align:top;float:left;display:inline;}
#daumBtnSearchAll:hover {background-position:0 bottom}

	if(str4==str3)  // 같은곳을 가르키니... 스트링은 객체니 주소값을 비교! 
	{
		System.out.println("같다");
	}else
	{
				System.out.println("다르다");
	}



	if(str4.equals(str3))  // 올래 주소를 비교하는 equals.
	{
		System.out.println("같다");
	}else
	{
				System.out.println("다르다");
	}


//string 클래스 사용한 비교문!! 
	String string1="EJB";

class String_StringBuffer{
	public static void main(String[] args){
		System.out.println("String_String buffer");


//String class
		String str1=new String("java");
		String str2="java";
		String str22="java";
		System.out.println(str2+str1);
		System.out.println("Wow".length());


	if(str2==str22)  // 같은곳을 가르키니... 스트링은 객체니 주소값을 비교! 
	{
		System.out.println("같다");
	}else
	{
				System.out.println("다르다");
	}



	if(str2.equals(str1))  // 안에 가진 값을 비교한다.  나름대로 고쳐사 사용
	{
		System.out.println("같다");
	}else
	{
				System.out.println("다르다");
	}

	if(str2==str1)  // 주소값을 비교한다/
	{
		System.out.println("같다");
	}else
	{
				System.out.println("다르다");
	}



//String Buffer class
		StringBuffer str3 =new StringBuffer("java");
